function CrearRegistro(texto) 
{
       let  RegistrodeNombre=prompt("Ingresa el nombre");
       let  RegistrodeCodigo=prompt("Ingresa el codigo,el codigo es la cantidad de palabras de la computadora");
       let  RegistrodeCantidad=prompt("Ingresa la cantidad");

       let Datos = {
       Nombre: RegistrodeNombre,
       Codigo: RegistrodeCodigo,
       Cantidad: RegistrodeCantidad,       
       }

       return Datos; 

       }


function codigo(Texto)  
{

       let  RegistrodeCodigo=prompt("Ingresa el codigo");
       let Codigo
       ={

       Codigo: RegistrodeCodigo,
       }

       return Codigo;

       }


       function cantidad(texto) {
